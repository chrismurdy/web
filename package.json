{
    "name": "demo-app",
    "version": "0.0.1",
    "scripts": {
        "start": "PORT=${PORT:-4000}; superstatic dist --port $PORT",
        "clean": "npm run clean_script",
        "dev": "npm run dev_script && node node_scripts/app-config.js media &&  concurrently \"ng build --dev -e takeover --delete-output-path false --watch\" \"superstatic dist --port 4000\"",
        "build:takeover": "npm run dev_script && node node_scripts/app-config.js takeover &&  concurrently \"ng build --dev -e takeover --delete-output-path false --watch\" \"superstatic dist --port 4000\"",
        "build:media": "npm run dev_script && node node_scripts/app-config.js media &&  concurrently \"ng build --dev -e media --delete-output-path false --watch\" \"superstatic dist --port 4000\"",
        "conf:media": "node node_scripts/app-config.js media",
        "conf:takeover": "node node_scripts/app-config.js takeover",
        "dev_script": "node node_scripts/dev.js",
        "clean_script": "node node_scripts/clean.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "superstatic": "latest"
    },
    "devDependencies": {
        "@angular/animations": "~5.2.6",
        "@angular/cli": "1.6.3",
        "@angular/common": "~5.2.6",
        "@angular/compiler": "~5.2.6",
        "@angular/compiler-cli": "~5.2.6",
        "@angular/core": "~5.2.6",
        "@angular/forms": "~5.2.6",
        "@angular/http": "~5.2.6",
        "@angular/language-service": "~5.2.6",
        "@angular/platform-browser": "~5.2.6",
        "@angular/platform-browser-dynamic": "~5.2.6",
        "@angular/platform-server": "~5.2.6",
        "@angular/router": "~5.2.6",
        "@angular-devkit/core": "~0.0.29",
        "@types/codemirror": "latest",
        "@types/core-js": "^0.9.46",
        "@types/d3": "^3.5.17",
        "@types/jquery": "^3.3.0",
        "@types/js-cookie": "^2.0.28",
        "@types/lodash": "^4.14.104",
        "@types/node": "^9.4.6",
        "@types/socket.io-client": "latest",
        "angular-in-memory-web-api": "^0.5.3",
        "browser-sync": "^2.23.6",
        "canonical-path": "latest",
        "concurrently": "latest",
        "copyfiles": "latest",
        "core-js": "latest",
        "es6-shim": "latest",
        "fs-extra": "latest",
        "ngx-bootstrap": "latest",
        "nodemon": "^1.15.1",
        "popper.js": "^1.13.0",
        "reflect-metadata": "^0.1.12",
        "rxjs": "5.5.6",
        "systemjs": "^0.21.0",
        "typescript": "~2.5.3",
        "zone.js": "latest"
    },
    "repository": {}
}
